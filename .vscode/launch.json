{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Client",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/client"
    },
    {
      "type": "node-terminal",
      "name": "Server",
      "request": "launch",
      "command": "npm run server",
      "cwd": "${workspaceFolder}"
    }
  ]
}
